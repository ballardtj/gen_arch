doi = {10.1037/0021-9010.92.4.928},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Schmidt, DeShon - 2007 - What to do The effects of discrepancies, incentives, and time on dynamic goal prioritization.pdf:pdf},
