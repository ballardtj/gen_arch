journal = {Developmental Cognitive Neuroscience},
keywords = {Adolescence,Development,Individual differences,Latent change scores,Longitudinal modelling,Structural equation modelling},
title = {{Developmental cognitive neuroscience using latent change score models: A tutorial and applications}},
url = {https://ac.els-cdn.com/S187892931730021X/1-s2.0-S187892931730021X-main.pdf?{\_}tid=53c85cf8-33ab-4a08-8818-f76a193520d9{\&}acdnat=1520383641{\_}68470a2ab4fd8749f404a57add58d0cf},
