journal = {Journal of occupational health psychology},
keywords = {Adaptation, Psychological,Adult,Employment,Employment: psychology,Female,Goals,Humans,Job Satisfaction,Longitudinal Studies,Male,Mental Fatigue,Multivariate Analysis,Netherlands,Nurses,Nurses: psychology,Regression Analysis,Self Efficacy},
month = {apr},
number = {2},
pages = {206--18},
pmid = {19331481},
title = {{Linking goal progress to subjective well-being at work: the moderating role of goal-related self-efficacy and attainability.}},
