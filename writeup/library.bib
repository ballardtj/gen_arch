journal = {Journal of Theoretical Biology},
keywords = {Communication networks,Decision making,Evolution,Gossip signaling,Information transmission},
pages = {152--160},
pmid = {23000073},
publisher = {Elsevier},
title = {{Making sense of information in noisy networks: Human communication, gossip, and distortion}},
url = {http://dx.doi.org/10.1016/j.jtbi.2012.09.009},
volume = {317},
year = {2013}
}
