@article{Heilman2010,
abstract = {It is well established that emotion plays a key role in human social and economic decision making. The recent literature on emotion regulation (ER), however, highlights that humans typically make efforts to control emotion experiences. This leaves open the possibility that decision effects previously attributed to acute emotion may be a consequence of acute ER strategies such as cognitive reappraisal and expressive suppression. In Study 1, we manipulated ER of laboratory-induced fear and disgust, and found that the cognitive reappraisal of these negative emotions promotes risky decisions (reduces risk aversion) in the Balloon Analogue Risk Task and is associated with increased performance in the prehunch/hunch period of the Iowa Gambling Task. In Study 2, we found that naturally occurring negative emotions also increase risk aversion in Balloon Analogue Risk Task, but the incidental use of cognitive reappraisal of emotions impedes this effect. We offer evidence that the increased effectiveness of cognitive reappraisal in reducing the experience of emotions underlies its beneficial effects on decision making.},
author = {Heilman, Renata M and Crişan, Liviu G and Houser, Daniel and Miclea, Mircea and Miu, Andrei C},
doi = {10.1037/a0018489},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Heilman et al. - 2010 - Emotion regulation and decision making under risk and uncertainty(2).pdf:pdf},
isbn = {1528-3542$\backslash$n1931-1516},
issn = {1931-1516},
journal = {Emotion},
keywords = {cognitive reappraisal,decision making,emotion regulation,expressive suppression,risk},
pages = {257--265},
pmid = {20364902},
title = {{Emotion regulation and decision making under risk and uncertainty}},
volume = {10},
year = {2010}
}
