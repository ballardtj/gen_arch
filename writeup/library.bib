isbn = {2122633255},
issn = {1095-9572},
journal = {NeuroImage},
keywords = {Bayes Theorem,Behavior,Behavior: physiology,Brain,Brain Mapping,Brain Mapping: methods,Brain: physiology,Cognition,Cognition: physiology,Computer Simulation,Humans,Magnetic Resonance Imaging,Models,Neurological},
