journal = {Sleep},
keywords = {Actigraphy,Adolescent,Adult,Choice Behavior,Female,Humans,Male,Psychological Tests,Reward,Sleep Deprivation,Sleep Deprivation: psychology,Time Factors,Wakefulness,Young Adult},
