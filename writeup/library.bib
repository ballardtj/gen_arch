journal = {NeuroImage},
keywords = {Adolescent,Adult,Affect,Affect: physiology,Brain Mapping,Corpus Striatum,Corpus Striatum: physiology,Decision Making,Decision Making: physiology,Female,Humans,Inhibition (Psychology),Male,Neuronal Plasticity,Neuronal Plasticity: physiology,Punishment,Reward,Young Adult},
month = {aug},
number = {1},
pages = {154--66},
pmid = {22548809},
publisher = {Elsevier Inc.},
title = {{Go and no-go learning in reward and punishment: interactions between affect and effect.}},
