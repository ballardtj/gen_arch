}
@article{Chapman2006,
abstract = {Two experiments were performed to test a psychophysical account of parallels between biases in risky choice and intertemporal choice. Experiment 1 demonstrated the common difference effect in intertemporal choice and the common ratio effect in risky choice. As was predicted, these two biases were uncorrelated with each other, although each was correlated across monetary/health domains. This result is consistent with the supposition that these two biases result from psychophysical properties of two different dimensions (time and probability, respectively). Experiment 2 examined the magnitude effect in intertemporal choice and the peanuts effect in risky choice. These two biases were correlated with each other but were uncorrelated across monetary/health domains. This result is consistent with the supposition that these two biases result from psychophysical properties of the same dimension (utility of money or health).},
author = {Chapman, Gretchen B and Weber, Bethany J},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Chapman, Weber - 2006 - Decision biases in intertemporal choice and choice under uncertainty testing a common account.pdf:pdf},
