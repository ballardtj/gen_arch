pages = {385989},
pmid = {30471996},
publisher = {Elsevier Ltd.},
title = {{Decision-making through integration of sensory evidence at prolonged timescales}},
url = {https://www.biorxiv.org/content/early/2018/08/07/385989},
year = {2018}
}
