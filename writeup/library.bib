journal = {Animal Behaviour},
month = {apr},
pages = {665--676},
pmid = {10792922},
title = {{Ways to test stochastic dynamic programming models empirically}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10792922},
volume = {59},
year = {2000}
}
