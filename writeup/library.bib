keywords = {Automation,Costs,Delegation,Human computer interaction,Human-Robot Interaction,Interfaces (computer),Interruption,Man machine systems,Military operations,Playbook,Task Switching,Unmanned Vehicles,Unmanned vehicles},
pages = {26--32},
title = {{Human control of multiple unmanned vehicles: Effects of interface type on execution and task switching times}},
