file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Mallis et al. - 2004 - Summary of the key features of seven biomathematical models of human fatigue and performance.pdf:pdf},
journal = {Aviation, Space, and Environmental Medicine},
