@article{Yechiam2018,
abstract = {The majority of the literature on the psychology of gains and losses suggests that losses lead to an avoidance response. Several studies, however, have shown that losses can also lead to an approach response, whereby an option is selected more often when it produces losses. In five studies we examine the boundary conditions for these contradictory approach and avoidance effects. The results show that an approach response emerges only when losses are produced by a highly advantageous choice alternative and when participants have ample unbiased direct or vicarious experience with this alternative. Addi- tionally, the avoidance response to losses is also not ubiquitous and emerges when alternatives producing losses are experienced as disadvantageous. Thus, the findings suggest that both the approach and avoidance effects of losses exist and can be accounted for by increased investment of cognitive resources with losses (i.e., loss attention). Additionally, the findings clarify the loss attention account in indicating that losses increase exploitative behavior based on experienced outcomes, a process which can be locally optimal. Keywords:},
author = {Yechiam, Eldad and Ashby, Nathaniel J. S. and Hochman, Guy},
doi = {10.1037/xlm0000607},
file = {:Users/uqtballa/Downloads/2018-32836-001.pdf:pdf},
issn = {0278-7393},
journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
keywords = {1971,a firm advocate of,asserted that they can,even,experience,have a positive effect,have complex consequences and,learning,loss attention,loss aversion,on classroom attention,penalties are known to,problem solving,skinner,the disutility of penalties},
number = {4},
pages = {591--605},
title = {{Are we attracted by losses? Boundary conditions for the approach and avoidance effects of losses.}},
volume = {45},
year = {2018}
}
@article{Heilman2010a,
abstract = {It is well established that emotion plays a key role in human social and economic decision making. The recent literature on emotion regulation (ER), however, highlights that humans typically make efforts to control emotion experiences. This leaves open the possibility that decision effects previously attributed to acute emotion may be a consequence of acute ER strategies such as cognitive reappraisal and expressive suppression. In Study 1, we manipulated ER of laboratory-induced fear and disgust, and found that the cognitive reappraisal of these negative emotions promotes risky decisions (reduces risk aversion) in the Balloon Analogue Risk Task and is associated with increased performance in the prehunch/hunch period of the Iowa Gambling Task. In Study 2, we found that naturally occurring negative emotions also increase risk aversion in Balloon Analogue Risk Task, but the incidental use of cognitive reappraisal of emotions impedes this effect. We offer evidence that the increased effectiveness of cognitive reappraisal in reducing the experience of emotions underlies its beneficial effects on decision making.},
author = {Heilman, Renata M and Crişan, Liviu G and Houser, Daniel and Miclea, Mircea and Miu, Andrei C},
doi = {10.1037/a0018489},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Heilman et al. - 2010 - Emotion regulation and decision making under risk and uncertainty.pdf:pdf},
issn = {1931-1516},
journal = {Emotion (Washington, D.C.)},
keywords = {Decision Making,Decision Making: physiology,Emotional Intelligence,Emotional Intelligence: physiology,Emotions,Emotions: physiology,Female,Humans,Male,Psychological Tests,Risk,Uncertainty,Young Adult},
month = {apr},
number = {2},
pages = {257--65},
pmid = {20364902},
title = {{Emotion regulation and decision making under risk and uncertainty.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20364902},
volume = {10},
year = {2010}
}
