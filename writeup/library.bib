doi = {10.1111/jsr.12483},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Rusterholz et al. - 2017 - Interindividual differences in the dynamics of the homeostatic process are trait-like and distinct for sleep.pdf:pdf},
isbn = {4144635570},
issn = {13652869},
journal = {Journal of Sleep Research},
keywords = {ICC,mathematical modeling,multiple recordings,phenotype,sleep/wake regulation,variance components},
number = {2},
pages = {171--178},
pmid = {28019041},
title = {{Interindividual differences in the dynamics of the homeostatic process are trait-like and distinct for sleep versus wakefulness}},
volume = {26},
year = {2017}
}
@article{Linde2009,
