doi = {10.1037/0096-1523.11.1.14},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Onken, Hastie, Revelle - 1985 - Individual differences in the use of simplification strategies in a complex decision-making task.pdf:pdf},
