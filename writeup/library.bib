journal = {Journal of Economic Psychology},
keywords = {Behavioral economics,Choice behavior,Decision making,Sport psychology,Uncertainty},
number = {5},
pages = {606--621},
title = {{Action bias among elite soccer goalkeepers: The case of penalty kicks}},
volume = {28},
year = {2007}
}
