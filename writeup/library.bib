issn = {0066-4308},
journal = {Annual Review of Psychology},
keywords = {Animals,Cognition,Conditioning (Psychology),Humans,Learning,Thinking},
pages = {273--301},
pmid = {19575617},
title = {{Learning: From association to cognition}},
url = {http://www.ncbi.nlm.nih.gov/sites/entrez?Db=pubmed{\&}DbFrom=pubmed{\&}Cmd=Link{\&}LinkName=pubmed{\_}pubmed{\&}LinkReadableName=Related Articles{\&}IdsFromResult=19575617{\&}ordinalpos=3{\&}itool=EntrezSystem2.PEntrez.Pubmed.Pubmed{\_}ResultsPanel.Pubmed{\_}RVDocSum},
volume = {61},
year = {2010}
}
