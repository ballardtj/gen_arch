@article{Porat2016,
abstract = {Proliferation in the use of Unmanned Aerial Systems (UASs) in civil and military operations has presented a multitude of human factors challenges; from how to bridge the gap between demand and availability of trained operators, to how to organize and present data in meaningful ways. Utilizing the Design Research Methodology (DRM), a series of closely related studies with subject matter experts (SMEs) demonstrate how the focus of research gradually shifted from "how many systems can a single operator control" to "how to distribute missions among operators and systems in an efficient way". The first set of studies aimed to explore the modal number, i.e., how many systems can a single operator supervise and control. It was found that an experienced operator can supervise up to 15 UASs efficiently using moderate levels of automation, and control (mission and payload management) up to three systems. Once this limit was reached, a single operator's performance was compared to a team controlling the same number of systems. In general, teams led to better performances. Hence, shifting design efforts toward developing tools that support teamwork environments of multiple operators with multiple UASs (MOMU). In MOMU settings, when the tasks are similar or when areas of interest overlap, one operator seems to have an advantage over a team who needs to collaborate and coordinate. However, in all other cases, a team was advantageous over a single operator. Other findings and implications, as well as future directions for research are discussed.},
author = {Porat, Talya and Oron-Gilad, Tal and Rottem-Hovev, Michal and Silbiger, Jacob},
doi = {10.3389/fpsyg.2016.00568},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Porat et al. - 2016 - Supervising and controlling unmanned systems A multi-phase study with subject matter experts.pdf:pdf},
issn = {16641078},
journal = {Frontiers in Psychology},
keywords = {Automation,Control ratio,DSS,Decision support systems,Human factors,Macrocognition,UAV,Unmanned aerial systems},
number = {MAY},
pmid = {27252662},
title = {{Supervising and controlling unmanned systems: A multi-phase study with subject matter experts}},
volume = {7},
year = {2016}
}
@article{Killeen2009,
abstract = {Goods remote in temporal, spatial, or social distance, or in likelihood, exert less control over our behavior than those more proximate. The decay of influence with distance, of perennial interest to behavioral economists, has had a renaissance in the study of delay discounting. By developing discount functions from marginal utilities, this article provides a framework that resolves several anomalies of intertemporal choice. Utilities are inferred to be power functions of monetary value, delay, and probability. Utility, not value, is discounted, with decisions made by adding the utility of a good to the disutility of a delay or contingency. The theory reduces to standard treatments, such as exponential, hyperbolic and hyperboloid, and exponential-power; naturally predicts magnitude effects and other asymmetries; is consistent with subadditivity, immediacy, and certainty effects; returns conjointly measured determinations of monetary utility and temporal distance functions; and is extensible to other dimensions of distance. Keywords: intertemporal choice, delay discount functions, dynamic inconsistency, magnitude effect, utility All problems of self-management involve establishing durable control by globally preferred goods-a long healthy life, college degree, promotion, attainment of heaven, or a legacy-in the face of smaller, more immediate temptations-rich food, sin, or cash settlements. When humans and other animals choose between delayed or distant commodities, they typically evince a number of peculiarities, among them: (a) They discount the future much more steeply than realistic; if offered a reliable {\$}100 in one year or an immediate settlement, for example, subjects will often settle for around {\$}70, implying an unrealistic 36{\%} compound discount rate. (b) Their discount functions are typically nonexponential. Exponential discount functions are the gold standard for valuation of deferred assets, with lenders compensated for deferring access to assets, as with certificates of deposit, by contracting an exponential increase in payout as a function of their period. When discount functions are not exponential they may show reversal of value over time: Subjects prefer a larger delayed good over a smaller, less delayed one, but as the passage of time makes the smaller one imminent, they impulsively take it. This is shown in a standard representation (see Figure 1) where a larger deferred good is preferred over a smaller, slightly more immediate reward-when both are temporally remote. As time passes, the difference in utility diminishes, until at time t , preference for the small immediate good becomes dominant. Exponential discount functions with the same discount rate will not cross at a point, as these do. These curves must either have different rate parameters, with the larger amount being discounted more slowly, or they must be of a different form; there is evidence for both conditions in the literature , as shown below. The discount functions that describe both verbal estimates of value and observed preference reversals are called hyper-bolic (v t v 0 /1 t, shown in Figure 1), or hyperboloid (v t v 0 /1 t ; Rachlin, 2006; Rodriguez {\&} Logue, 1988). The difficulties of understanding intertemporal preferences theoretically , and their signal importance for both personal and political economy, are well-described by Loewenstein and Elster (1992), with Madden and Bickel (2009) providing a current overview. Hyperbolic discount functions entail a more complicated actuarial system than the exponential: The discount rate is continually changing, with the consumer projecting a discount rate for a future epoch (e.g., next year) that is less than he or she will project for it at any subsequent time up to the start of that epoch. It is not just that the valuation during the year changes as the year progresses; the proportion by which it changes is also continuously changing. In contrast, the exponential discounter is consistent in applying the same rate at all times: His or her decrement depends only on the length of the epoch, not its remoteness from the decision point. The present research was motivated by the desire to simplify the imputed cognitive economics of decision makers, to rationalize it, and to provide a unified analytic tool for researchers. The present account starts from the most fundamental representation of devaluation, the marginal rate of discounting. It does so for several reasons. The marginal rate, or derivative of the discount function, is the heart of the "diminishing marginal utility" of Bernoulli, and identifies the marginal rate (Equation 1) that leads uniquely to exponential discount functions. The marginal rate gives birth to competing models of temporal discounting from a fundamental and intuitive level. Its integration between specific time (t) limits t 1 and t 2 gives the discount factor between those times, , which is a key object of many recent economic treatises. And finally, it starts with marginal rates because simple generalizations of the most basic marginal discount rate (Equation 1) led to a novel and accurate discount function that would not have been otherwise discovered.},
author = {Killeen, Peter R},
doi = {10.1037/a0016414},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Killeen - 2009 - An additive-utility model of delay discounting.pdf:pdf},
journal = {Psychological Review},
pages = {602--619},
title = {{An additive-utility model of delay discounting}},
volume = {116},
year = {2009}
}
