journal = {Journal of Experimental Psychology: Applied},
keywords = {analytic processing,considered and integrated,coordination of diagnostic processes,information of,medical cognition,medical education,of medical diagnosis,researchers have,similarity-based processing,to accomplish the task,various types must be},
pages = {195--209},
pmid = {21942311},
title = {{Teaching posttraining: Influencing diagnostic strategy with instructions at test.}},
