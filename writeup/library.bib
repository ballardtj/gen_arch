journal = {Psychological Bulletin},
keywords = {10,1037,a0025940,cognitive bias and heuristic,decisions,doi,dx,http,infor-,judgment and decision making,mation theory,org,subjective probabilities,supp,supplemental materials,theoretical framework,they essentially make a,when human judges make},
number = {2},
pages = {211--237},
pmid = {22122235},
title = {{Toward a synthesis of cognitive biases: How noisy information processing can bias human decision making.}},
volume = {138},
year = {2012}
