issn = {1873-6327},
journal = {Addictive behaviors},
keywords = {Adult,Female,Humans,Male,Motivation,Smoking,Smoking Cessation,Smoking Cessation: methods,Smoking Cessation: psychology,Smoking: psychology},
month = {may},
