journal = {Human factors},
keywords = {HUMAN SUPERVISORY CONTROL,MANNING,UNMANNED VEHICLES,WORKLOAD},
number = {1},
pages = {1--15},
pmid = {17315838},
title = {{Developing operator capacity estimates for supervisory control of autonomous vehicles.}},
volume = {49},
year = {2007}
