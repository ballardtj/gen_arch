@article{Johnson1981,
abstract = {We reviewed 122 studies and compared the relative effectiveness of cooperation, cooperation with intergroup competition, interpersonal competition, and individ-ualistic goal structures in promoting achievement and productivity in North American samples. These studies yielded 286 findings. Three meta-analysis pro-cedures were used: voting method, effect-size method, and z-scorc method. The results of the meta-analyses indicate (a) that cooperation is considerably more effective than interpersonal competition and individualistic efforts, (b) that co-operation with intergroup competition is also superior to interpersonal compe-tition and individualistic efforts, and (c) that there is no significant difference between interpersonal competitive and individualistic efforts. Through multiple regression, a number of potentially mediating variables for these results are identified. Since the 1920s there has been a great deal of research on the relative effects of cooperative, competitive, and individualistic efforts on achievement and productivity. Despite the large number of studies con-ducted, social scientists disagree as to the conclusions that may be drawn from the lit-erature. Several psychologists have reviewed subsets of studies within the area and have arrived at contradictory conclusions. There is therefore a need to review all the research literature to resolve the controversies. This article is an attempt to do so through the use of meta-analysis, Definition of Goal Structures Four goal structures are commonly stud-ied: (a) cooperation, (b) cooperation with intergroup competition, (c) interpersonal competition, and (d) individualistic efforts. There are two major approaches to defining these conditions, one evolving from the in-trinsic motivation point of view of Lewin's (1935) field theory, the other from the ex-trinsic motivation point of view of behavioral learning theory.},
author = {Johnson, David W and Maruyama, Geoffrey and Johnson, Roger and Nelson, Deborah and Skon, Linda},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Johnson et al. - 1981 - Effects of cooperative, competitive, and individualistic goal structures on achievement A meta-analysis.pdf:pdf},
journal = {Psychological Bulletin},
pages = {47--62},
title = {{Effects of cooperative, competitive, and individualistic goal structures on achievement: A meta-analysis}},
volume = {89},
year = {1981}
}
