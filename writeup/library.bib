doi = {10.1037/0021-9010.91.4.900},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Kozlowski, Bell - 2006 - Disentangling achievement orientation and goal setting effects on self-regulatory processes.pdf:pdf},
