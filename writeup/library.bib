journal = {Behavioural brain research},
keywords = {Adolescent,Adult,Avoidance Learning,Avoidance Learning: physiology,Brain Mapping,Brain Mapping: methods,Brain Mapping: psychology,Corpus Striatum,Corpus Striatum: physiology,Female,Frontal Lobe,Frontal Lobe: physiology,Humans,Learning,Learning: physiology,Limbic System,Limbic System: physiology,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Magnetic Resonance Imaging: psychology,Male,Middle Aged,Neural Pathways,Neural Pathways: physiology,Psychomotor Performance,Psychomotor Performance: physiology,Reinforcement (Psychology)},
month = {dec},
number = {2},
