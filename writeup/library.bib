@article{Verdonck2016,
abstract = {Choice reaction time (RT) experiments are an invaluable tool in psychology and neuroscience. A common assumption is that the total choice response time is the sum of a decision and a nondecision part (time spent on perceptual and motor processes). While the decision part is typically modeled very carefully (commonly with diffusion models), a simple and ad hoc distribution (mostly uniform) is assumed for the nondecision component. Nevertheless, it has been shown that the misspecification of the nondecision time can severely distort the decision model parameter estimates. In this article, we propose an alternative approach to the estimation of choice RT models that elegantly bypasses the specification of the nondecision time distribution by means of an unconventional convolution of data and decision model distributions (hence called the D*M approach). Once the decision model parameters have been estimated, it is possible to compute a nonparametric estimate of the nondecision time distribution. The technique is tested on simulated data, and is shown to systematically remove traditional estimation bias related to misspecified nondecision time, even for a relatively small number of observations. The shape of the actual underlying nondecision time distribution can also be recovered. Next, the D*M approach is applied to a selection of existing diffusion model application articles. For all of these studies, substantial quantitative differences with the original analyses are found. For one study, these differences radically alter its final conclusions, underlining the importance of our approach. Additionally, we find that strongly right skewed nondecision time distributions are not at all uncommon. (PsycINFO Database Record (c) 2015 APA, all rights reserved)},
author = {Verdonck, Stijn and Tuerlinckx, Francis},
doi = {10.1037/rev0000019},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Verdonck, Tuerlinckx - 2016 - Factoring out nondecision time in choice reaction time data Theory and implications.pdf:pdf},
issn = {1939-1471(Electronic);0033-295X(Print)},
journal = {Psychological Review},
keywords = {10,1037,bias,choice rt,d ء m,diffusion model,doi,dx,http,misspecification,nondecision time,org,rev0000019,supp,supplemental materials},
title = {{Factoring out nondecision time in choice reaction time data: Theory and implications}},
volume = {123},
year = {2016}
}
@article{Zhang2018,
abstract = {In the current study, extending from the cross-lagged panel models (CLPMs) in Cole and Maxwell (2003), we proposed the multilevel autoregressive mediation models (MAMMs) by allowing the coefficients to differ across individuals. In addition, Level-2 covariates can be included to explain the interindividual differences of mediation effects. Given the complexity of the proposed models, Bayesian estimation was used. Both a CLPM and an unconditional MAMM were fitted to daily diary data. The 2 models yielded different statistical conclusions regarding the average mediation effect. A simulation study was conducted to examine the estimation accuracy of Bayesian estimation for MAMMs and consequences of model mis-specifications. Factors considered included the sample size (N), number of time points (T), fixed indirect and direct effect sizes, and Level-2 variances and covariances. Results indicated that the fixed effect estimates for the indirect effect components (a and b) and the fixed effects of Level-2 covariates were accurate when N Ն 50 and T Ն 5. For estimating Level-2 variances and covariances, they were accurate provided a sufficiently large N and T (e.g., N Ն 500 and T Ն 50). Estimates of the average mediation effect were generally accurate when N Ն 100 and T Ն 10, or N Ն 50 and T Ն 20. Furthermore, we found that when Level-2 variances were zero, MAMMs yielded valid inferences about the fixed effects, whereas when random effects existed, CLPMs had low coverage rates for fixed effects. DIC can be used for model selection. Limitations and future directions were discussed. Translational Abstract We propose the multilevel autoregressive mediation models (MAMMs) for longitudinal mediation analysis by allowing mediation effects to differ across individuals while accounting for the temporal sequence in the data. Person-level variables can be included to explain interindividual differences in mediation effects. Given the complexity of the proposed MAMMs, Bayesian estimation is used. A real data analysis revealed that inferences about the average mediation effect differed using an existing longitudinal mediation model assuming the same mediation effect across individuals and MAMMs. A simulation study was conducted to examine whether and when Bayesian estimation for MAMMs had accurate estimates and inferences of model parameters, and consequences of model mis-specifications. Results showed that when the number of indi-viduals (N) was greater than or equal to 50, and the number of time points (T) was greater than or equal to 5, the average mediation effect components (a and b) and direct effect (c) were accurately estimated and inferred. The same conclusion held for estimating and inferring the effects of person-level variables. Estimates of the average mediation effect were generally accurate when N Ն 100 and T Ն 10, or N Ն 50 and T Ն 20. Furthermore, ignoring interindividual differences in mediation effects may result in misleading inferences of a, b, c, and/or the average mediation effect. In contrast, assuming heterogeneous mediation effects across individuals whereas they did not exist in reality had minimal influences on inferences of a, b, c, and the average mediation effect. Limitations and future directions are discussed.},
author = {Zhang, Qian and Wang, Lijuan and Bergeman, C S},
doi = {10.1037/met0000161},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Zhang, Wang, Bergeman - 2018 - Multilevel autoregressive mediation models Specification, estimation, and applications.pdf:pdf},
journal = {Psychological Review},
pages = {278--297},
title = {{Multilevel autoregressive mediation models: Specification, estimation, and applications}},
volume = {23},
year = {2018}
}
