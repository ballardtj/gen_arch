keywords = {Accidents, Occupational,Accidents, Occupational: prevention {\&} control,Accidents, Occupational: statistics {\&} numerical da,Communication,Female,Humans,Leadership,Male,Pennsylvania,Personnel Management,Psychological Theory,Regression Analysis,Safety Management,Safety Management: methods,Wood},
