pages = {11--37},
title = {{Predicting operator capacity for supervisory control of multiple UAVs previous experimental multiple UAV studies}},
url = {http://link.springer.com/chapter/10.1007/978-3-540-72696-8{\_}2{\#}},
volume = {37},
