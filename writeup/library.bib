@article{Tucker2010,
abstract = {STUDY OBJECTIVES: We studied the effects of sleep deprivation on executive functions using a task battery which included a modified Sternberg task, a probed recall task, and a phonemic verbal fluency task. These tasks were selected because they allow dissociation of some important executive processes from non-executive components of cognition.$\backslash$n$\backslash$nDESIGN: Subjects were randomized to a total sleep deprivation condition or a control condition. Performance on the executive functions task battery was assessed at baseline, after 51 h of total sleep deprivation (or no sleep deprivation in the control group), and following 2 nights of recovery sleep, at fixed time of day (11:00). Performance was also measured repeatedly throughout the experiment on a control task battery, for which the effects of total sleep deprivation had been documented in previously published studies.$\backslash$n$\backslash$nSETTING: Six consecutive days and nights in a controlled laboratory environment with continuous behavioral monitoring.$\backslash$n$\backslash$nPARTICIPANTS: Twenty-three healthy adults (age range 22-38 y; 11 women). Twelve subjects were randomized to the sleep deprivation condition; the others were controls.$\backslash$n$\backslash$nRESULTS: Performance on the control task battery was considerably degraded during sleep deprivation. Overall performance on the modified Sternberg task also showed impairment during sleep deprivation, as compared to baseline and recovery and compared to controls. However, two dissociated components of executive functioning on this task--working memory scanning efficiency and resistance to proactive interference--were maintained at levels equivalent to baseline. On the probed recall task, resistance to proactive interference was also preserved. Executive aspects of performance on the phonemic verbal fluency task showed improvement during sleep deprivation, as did overall performance on this task.$\backslash$n$\backslash$nCONCLUSION: Sleep deprivation affected distinct components of cognitive processing differentially. Dissociated non-executive components of cognition in executive functions tasks were degraded by sleep deprivation, as was control task performance. However, the executive functions of working memory scanning efficiency and resistance to proactive interference were not significantly affected by sleep deprivation, nor were dissociated executive processes of phonemic verbal fluency performance. These results challenge the prevailing view that executive functions are especially vulnerable to sleep loss. Our findings also question the idea that impairment due to sleep deprivation is generic to cognitive processes subserved by attention.},
author = {Tucker, Adrienne M. and Whitney, Paul and Belenky, Gregory and Hinson, John M. and {Van Dongen}, Hans P.A.},
doi = {10.1093/sleep/33.1.47},
file = {:Users/uqtballa/Library/Application Support/Mendeley Desktop/Downloaded/Tucker et al. - 2010 - Effects of sleep deprivation on dissociated components of executive functioning.pdf:pdf},
isbn = {0161-8105 (Print) 0161-8105 (Linking)},
issn = {15509109},
journal = {Sleep},
keywords = {Cognitive performance,Digit-symbol substitution task (DSST),Executive functions,Probed recall,Psychomotor vigilance test (PVT),Sternberg task,Subjective sleepiness,Total sleep deprivation,Verbal fluency,Working memory},
pages = {47--57},
